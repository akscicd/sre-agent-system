#!/bin/bash

sudo apt-get update
sudo apt-get install -y openjdk-21-jre

curl -sO http://10.0.0.10:8080/jnlpJars/agent.jar
sudo java -jar agent.jar -url http://10.0.0.10:8080/ -secret <secret value should be added and angle brackets can be removed.> -name "akscicd-jenkins-agent" -webSocket -workDir "/var/jenkins"